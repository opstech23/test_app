<h1>Sign Up</h1>

<%= link_to "Home", root_path %>

<% user = "" %>
<%= form_for user do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br>
	<%= f.label :email %>
	<%= f.email_field :email %><br>
	<%= f.label :password %>
	<%= f.password_field :password %><br>
	<%= f.submit %>
<% end %>

<%= link_to "Log In", new_user_session %>